package com.morlink.dev.beans;

import java.util.ArrayList;
import java.util.Date;
import java.util.List;



public class Devis {
	
	private String ndevis;
	private Date dateCreation;
	private String nClient;
	private String IntituleClient;
	private int nbrRelanceClient;
	private String pj;
	private String ice;
	private List<LigneDevis> ligneDevises = new ArrayList<LigneDevis>();

	public String getNdevis() {
		return this.ndevis;
	}

	public void setNdevis(String ndevis) {
		this.ndevis = ndevis;
	}

	public Date getDateCreation() {
		return this.dateCreation;
	}

	public void setDateCreation(Date dateCreation) {
		this.dateCreation = dateCreation;
	}

	public String getNClient() {
		return this.nClient;
	}

	public void setNClient(String nClient) {
		this.nClient = nClient;
	}

	public int getNbrRelanceClient() {
		return this.nbrRelanceClient;
	}

	public void setNbrRelanceClient(int nbrRelanceClient) {
		this.nbrRelanceClient = nbrRelanceClient;
	}

	public String getPj() {
		return this.pj;
	}

	public void setPj(String pj) {
		this.pj = pj;
	}

	public List<LigneDevis> getLigneDevises()
	{
		return ligneDevises;
	}

	public void setLigneDevises(List<LigneDevis> ligneDevises)
	{
		this.ligneDevises = ligneDevises;
	}

	public String getIntituleClient()
	{
		return IntituleClient;
	}

	public void setIntituleClient(String intituleClient)
	{
		IntituleClient = intituleClient;
	}

	public String getIce() {
		return ice;
	}

	public void setIce(String ice) {
		this.ice = ice;
	}
}